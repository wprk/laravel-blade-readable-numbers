{
    "name": "wprk/laravel-blade-readable-numbers",
    "description": "A blade directive for human readable numbers, k-style.",
    "keywords": [
        "laravel-blade-readable-numbers",
        "laravel",
        "blade",
        "human-readable"
    ],
    "homepage": "https://github.com/wprk/laravel-blade-readable-numbers",
    "license": "MIT",
    "require": {
        "illuminate/support": "^5|^6|^7|^8",
        "php": "^5|^7|^8"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Wprk\\ReadableNumbers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wprk\\Tests\\ReadableNumbers\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wprk\\ReadableNumbers\\ReadableNumbersServiceProvider"
            ]
        }
    }
}
